{
	"compilerOptions": {
		"forceConsistentCasingInFileNames": true,
		"jsx": "react",
		"lib": ["es2015", "dom", "es2015.promise"],
		"module": "commonjs",
		"noFallthroughCasesInSwitch": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"outDir": "../../build/release/client",
		"pretty": true,
		"removeComments": true,
		"sourceMap": true,
		"strictNullChecks": true,
		"target": "es5",
		"typeRoots": [
			"../types/client",
			"../types/shared",
			"../../node_modules/@types"
		],
		"types": [
			"webpack-env"
		]
	},
	"files": [
		"app.tsx"
	]
}
